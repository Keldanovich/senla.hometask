			//System.out.print("¬ведите значение");
			//mas[i]= Integer.parseInt(reader.readLine());
			mas[i]=i;
			System.out.println(mas[i]);}
			for (int i = 1; i<n; i++)
			if (mas[i]==1 || mas[i]==2 || mas[i]==3) {System.out.println(mas[i]);}
			//for(int d=2; d*d<=mas[i]; d++){
